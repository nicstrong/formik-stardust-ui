{
  "name": "formik-startdust-ui",
  "description": "Formik wrapper of stardust-ui form components",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Nic Strong <nic.strong@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicstrong/formik-stardust-ui/"
  },
  "keywords": [
    "formik",
    "react",
    "startdust-ui"
  ],
  "main": "dist/index.js",
  "module": "dist/formik-startdust-ui.esm.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/**/*' 'test/**/*' 'README.md'"
  },
  "peerDependencies": {
    "react": ">= 16.8.0 < 17",
    "react-dom": ">= 16.8.0 < 17",
    "formik": ">= 1.5.0 < 2",
    "@stardust-ui/react": "> 0.36.0 < 1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/react": "16.9.1",
    "@types/react-dom": "16.8.5",
    "@stardust-ui/react": "^0.36.1",
    "formik": "^1.5.8",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "typescript": "3.5.3",
    "husky": "^3.0.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "tsdx": "^0.7.2",
    "tslib": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
